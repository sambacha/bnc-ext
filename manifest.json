{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Blocknative ETH Gas Estimator",
  "description": "Get into the next block without overpaying. Accurate Ethereum gas estimations.",
  "version": "0.3.5",
  "manifest_version": 2,
  "icons": {
    "48": "icons/bn-logo-chrome-48.png",
    "128": "icons/bn-logo-chrome-128.png"
  },
  "background": {
    "scripts": ["build/background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
